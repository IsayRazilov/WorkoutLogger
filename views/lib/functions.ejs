<%
GetColorByWorkout = function(workoutType) {
    color = "";
    switch(workoutType) {
        case "Deadlift":
            color = "rgba(160, 28, 28);" 
            break
        case "Benchpress":
            color = "rgba(3, 122, 3);" 
            break
        case "Squat":
            color = "rgba(17, 17, 161);" 
            break
    }
    return color;
}
%>